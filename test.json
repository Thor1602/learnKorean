{
  "subject_marker": [
    {
      "Topic_Kor": "-가/-이",
      "Description_Eng": "Subject Marker",
      "Rules": [
        "Subject ends with a vowel: +이",
        "Subject ends with a consonant: +가"
      ],
      "Examples": [
        "의사가 사과를 먹어요. The doctor eats an apple.",
        "선생님이 배를 먹어요. The teacher eats a pear"
      ]
    }
  ],
  "object_marker": [
    {
      "Topic_Kor": "-을/-를",
      "Description_Eng": "Object Marker",
      "Rules": [
        "Object ends with a vowel: + 를",
        "Object ends with a consonant: + 을"
      ],
      "Examples": [
        "저는 책을 읽어요. I read a book.",
        "그는 커피를 사요. He buys coffee."
      ]
    }
  ],
